.projectDetailBox {
  padding: 32rpx;
  background: #fff;
}

.projectDetailTop {
  margin-bottom: 32rpx;
  position: relative;
}

.belongTo {
  position: absolute;
  top: -16rpx;
  left: -18rpx;
  width: 116rpx;
  height: 36rpx;
}

.belongTo1 { 
  position: absolute;
  top: 22rpx;
  left: -18rpx;
  width: 116rpx;
  height: 36rpx;
}

.projectDetailLeft {
  width: 104rpx;
  height: 104rpx;
  margin-right: 24rpx;
  margin-bottom: 8rpx;
}

.projectDetailTitle {
  font-size: 32rpx;
  color: #333;
  text-align: left;
  line-height: 44rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 558rpx;
}

.projectDetailContent {
  max-width: 558rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-size: 24rpx;
  line-height: 34rpx;
  color: #999;
  text-align: left;
}

.projectDetailCenter {
  font-size: 24rpx;
  color: #666;
  text-align: left;
  line-height: 34rpx;
}

.projectSpecial {
  word-break: break-all;
  flex-wrap: wrap;
  max-width: 556rpx;
}

.t-roundGroupTitle {
  width: 24rpx;
  height: 24rpx;
  margin-right: 22rpx;
}

.stageBox {
  margin: 20rpx 0 32rpx 0;
}

.otherProjectBox {
  margin-bottom: 14rpx;
}

.otherProject {
  background: rgba(51, 120, 224, 0.10);
  border-radius: 4rpx;
  padding: 6rpx 12rpx 8rpx 12rpx;
  margin-right: 24rpx;
  font-size: 24rpx;
  color: #3378e0;
  margin-bottom: 8rpx;
  /* white-space: nowrap; */
}

.userInfo {
  background: #fff;
  padding: 32rpx;
  border-top: 1rpx solid #ccc;
}

.userPicText {
  width: 80rpx;
  height: 80rpx;
  margin-right: 16rpx;
  border-radius: 50%;
  line-height: 80rpx;
}

.userPic {
  width: 80rpx;
  height: 80rpx;
  margin-right: 16rpx;
  border-radius: 50%;
}

#userInfoRight .userName {
  font-size: 28rpx;
  color: #333;
  line-height: 40rpx;
  text-align: left;
  margin-bottom: 6rpx;
}

#userInfoRight .userCompany {
  font-size: 28rpx;
  color: #666;
  line-height: 34rpx;
  text-align: left;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-height: 554rpx;
}

.companyIntro {
  background: #fff;
  padding: 32rpx;
  padding-bottom: 0;
  margin-bottom: 32rpx;
}

.companyName {
  font-size: 30rpx;
  color: #333;
  text-align: left;
  margin-bottom: 32rpx;
  font-weight: bold;
}

.detailTitle {
  font-size: 30rpx;
  color: #333;
  line-height: 42rpx;
  text-align: left;
  font-weight: bold;
  padding: 32rpx 0;
}

.detailCompanyName {
  background: #f9fafc;
  border-radius: 4rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 28rpx;
  color: #333;
  text-align: left;
  padding: 24rpx;
  width: 470rpx;
  margin-right: 32rpx;
  margin-bottom: 32rpx;
}

.detailOneDetail {
  font-size: 24rpx;
  color: #ccc;
  text-align: center;
  padding: 10rpx 15rpx;
  border: 1rpx solid #ccc;
  border-radius: 4rpx;
  height: 36rpx;
}

.detailContent {
  font-size: 28rpx;
  color: #666;
  line-height: 44rpx;
  text-align: left;
}

.clickAll {
  background: #fafafa;
  font-size: 20rpx;
  color: #999;
  text-align: center;
  padding: 12rpx;
  width: 100%;
}

.companypPoint, .companypBook, .companyMainProject, .companyHistory,
.companyCorePerson, .companyStone, .similarProject, .mediaReport {
  padding: 0 32rpx;
  background: #fff;
  margin-bottom: 32rpx;
}

.otherContent {
  padding-bottom: 32rpx;
}

.bookPoint {
  background: #40587a;
  width: 12rpx;
  height: 12rpx;
  display: block;
  border-radius: 100%;
  margin-right: 12rpx;
  position: relative;
  top: 18rpx;
}

.bookBlockContent {
  border-left: 8rpx solid rgba(64, 88, 122, 0.2);
  margin-left: 24rpx;
}

.bookBlockTitle {
  font-size: 32rpx;
  color: #333;
  margin: 32rpx 0;
}

.bookReason {
  font-size: 28rpx;
  color: #333;
  text-align: left;
  margin: 0 32rpx 0 36rpx;
  white-space: nowrap;
}

.bookReasonContent {
  font-size: 28rpx;
  color: #666;
  text-align: left;
  line-height: 40rpx;
}

.bookContentPadding {
  margin-bottom: 48rpx;
}

.contentPadding {
  padding-bottom: 20rpx;
}

/*主要产品  */

.mainProjectBox, .historyBox, .corePersonBox, .similarProjectBox,
.mediaReportBox {
  padding: 32rpx 0;
  border-bottom: 1rpx solid #f0f0f0;
}

.title {
  justify-content: space-between;
}

.littleTitle {
  font-size: 30rpx;
  color: #333;
  text-align: left;
  line-height: 42rpx;
  margin-bottom: 8rpx;
}

.allNumber {
  font-size: 24rpx;
  color: #3378e0;
  text-align: right;
  position: relative;
  top: 36rpx;
}

.projectImage {
  height: 104rpx;
  width: 104rpx;
  margin-right: 32rpx;
}

.detailType, .similarScale {
  border: 1rpx solid #40587a;
  border-radius: 68rpx;
  font-size: 20rpx;
  line-height: 46rpx;
  padding: 0 12rpx;
  color: #40587a;
  /* border-radius: 98rpx; */
  text-align: center;
  width: auto;
  flex-wrap: nowrap;
  white-space: nowrap;
}

.mainProject {
  font-size: 24rpx;
  color: #999;
  text-align: left;
  line-height: 34rpx;
  max-width: 552rpx;
}

.mainProjectBox:last-child {
  border-bottom: none;
}

/*历史融资  */

.historyBox {
  border-bottom: none;
}

.left {
  text-align: right;
  width: 18%;
  padding-right: 22rpx;
  border-right: 1rpx solid #ccc;
}

.right {
  width: 82%;
  margin-left: 22rpx;
}

.mainProjectTime, .mainProjectScale, .mainProjectMoney, .mainProjectOther {
  font-size: 28rpx;
  color: #666;
  line-height: 40rpx;
}

.mainProjectTime {
  margin-bottom: 12rpx;
}

.mainProjectScale {
  color: #333;
  text-align: right;
}

.mainProjectMoney {
  color: #f6a623;
  text-align: left;
  margin-bottom: 12rpx;
}

.mainProjectOther {
  text-align: left;
  color: #333;
  margin-bottom: 12rpx;
}

