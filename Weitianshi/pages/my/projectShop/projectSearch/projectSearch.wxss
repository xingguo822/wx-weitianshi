.search_frame {
  position: fixed;
  height: 56rpx;
  width: 94%;
  background: white;
  padding: 16rpx 128rpx 16rpx 32rpx;
  z-index: 5;
}

.searchBox {
  align-items: center;
  height: 56rpx;
  width: 590rpx;
  line-height: 56rpx;
  background: #f2f2f2;
  border-radius: 8rpx;
  float: left;
}

.search {
  margin: 0;
  font-size: 28rpx;
  height: 56rpx;
  width: 85%;
  line-height: 56rpx;
  padding-left: 24rpx;
}

/*取消*/

.esc {
  /*clear:both;*/
  float: right;
  font-size: 32rpx;
  color: #40587a;
  margin-top: 14rpx;
  margin-right: 32rpx;
}

.outBox {
  background: white;
  width: 100%;
  height: auto;
}

.insideBox {
  border-bottom: 0.5rpx solid #f0f0f0;
  padding: 32rpx;
  position: relative;
}

.insideBox:last-child {
  border-bottom: none;
}

.outBox .describle {
  font-size: 30rpx;
  color: #333;
  line-height: 42rpx;
  text-align: left;
  margin-bottom: 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  font-weight: bold;
}

.projectDetailType {
  height: 92rpx;
  width: 92rpx;
  right: -3rpx;
  top: -6rpx;
  position: absolute;
}

.allIndustry {
  background: #f5f5f5;
  border-radius: 4rpx;
  width: 128rpx;
  height: 50rpx;
  font-size: 24rpx;
  color: #666;
  line-height: 50rpx;
  text-align: center;
  margin-right: 16rpx;
  margin-bottom: 20rpx;
  white-space: nowrap;
}

.projectDetailLine {
  font-size: 24rpx;
  line-height: 32rpx;
  text-align: left;
  padding-left: 10rpx;
  color: #666;
}

.projectDetailLine:first-child {
  padding-left: 0;
}

.projectDetailLine:last-child {
  border-right: none;
}

.allProjectDetail {
  font-size: 26rpx;
  color: #666;
  line-height: 26rpx;
  text-align: left;
}

.industryBox {
  word-break: break-all;
  flex-wrap: wrap;
}

.otherInfo {
  position: relative;
}

.others_pic {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-right: 16rpx;
}

.user_image {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-right: 16rpx;
  border-radius: 50%;
  font-size: 22rpx;
  background: #f2f2f2;
  line-height: 40rpx;
  color: #40587a;
}

.userInfo {
  width: 566rpx;
  margin-top: 9rpx;
}

.userCompany, .userCareer, .userName {
  font-size: 22rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  width: 320rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.userCareer, .userName {
  margin-right: 16rpx;
  white-space: nowrap;
  width: auto;
  text-overflow: clip;
  overflow: none;
}

.star {
  height: 52rpx;
  width: 112rpx;
  position: absolute;
  right: 26rpx;
  bottom: 24rpx;
}

.starPic {
  height: 52rpx;
  width: 112rpx;
}

.picFather {
  position: relative;
}

.user_pic {
  height: 104rpx;
  width: 104rpx;
  border-radius: 50%;
  position: absolute;
  right: -220rpx;
  top: -4rpx;
}

.user_text {
  height: 104rpx;
  width: 104rpx;
  border-radius: 50%;
  line-height: 104rpx;
  position: absolute;
  right: -220rpx;
  top: -4rpx;
}

.topPic {
  height: 46rpx;
  width: 64rpx;
}

.top {
  height: 46rpx;
  width: 64rpx;
  position: absolute;
  right: 26rpx;
  bottom: 30rpx;
}

.nullState {
  position: absolute;
  top: 642rpx;
  left: 302rpx;
  font-size: 36rpx;
  color: #999;
}
