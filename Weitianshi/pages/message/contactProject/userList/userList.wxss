/* pages/message/contactProject/userList/userList.wxss */
.browseMeTitle {
  /*height: 90rpx;*/
  margin: 32rpx 32rpx 32rpx 32rpx;
}

.browseMeTitle .num {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: right;
}

.title {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  text-align: left;
}

.title .block {
  background: #40587a;
  width: 4rpx;
  height: 24rpx;
  margin-right: 12rpx;
}
.t-projectInfo {
  display:none;
}
.t-otherInfo {
  position: relative;
   padding-bottom: 0;
  border:none;
}
.t-outBox .t-describle {
  font-size: 34rpx;
  color: #333;
  line-height: 38rpx;
  text-align: left;
  margin-bottom: 80rpx;
  /* width:1418rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}


.t-insideBox {
  padding: 40rpx 32rpx 40rpx 32rpx;
  background: #fff;
  border-radius: 8rpx;
}

.noData {
  width: 144rpx;
  height: 88rpx;
  margin-bottom: 38rpx;
  /* margin-top:344rpx; */
}
.hasNothing {
  color: #666;
}
