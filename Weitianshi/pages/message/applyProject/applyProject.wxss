  padding: 32rpx;
}

.outsideBox {
  margin: 0 30rpx;
  position: relative;
}

#introduce {
  background: #fafafa;
  border-radius: 4rpx 4rpx 0 0;
  font-size: 26rpx;
  color: #666;
  line-height: 88rpx;
  text-align: left;
  height: 88rpx;
  margin-left: 30rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 691rpx;
