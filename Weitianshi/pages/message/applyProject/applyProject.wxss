/* pages/message/applyProject/applyProject.wxss */

page {
  background: #f3f4f8;
}

.swiper-tab {
  width: 100%;
  text-align: center;
  line-height: 80rpx;
  background: #fff;
}

.swiper-tab-list {
  display: inline-block;
  width: 50%;
  font-size: 26rpx;
  color: #999;
  line-height: 62rpx;
  text-align: center;
  border: none;
}

.on {
  color: #40587a;
  text-align: center;
  font-size: 26rpx;
  line-height: 80rpx;
  border-bottom: 4rpx solid #40587a;
}

.empty {
  padding-top: 0;
  margin-top: 344rpx;
  width: 252rpx;
  height: 190rpx;
}

.browseMeTitle {
  /*height: 90rpx;*/
  margin: 32rpx 32rpx 32rpx 32rpx;
}

.browseMeTitle .num {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: right;
}

.title {
  font-size: 26rpx;
  color: #333;
  line-height: 26rpx;
  text-align: left;
}

.title .block {
  background: #40587a;
  width: 4rpx;
  height: 24rpx;
  margin-right: 12rpx;
}

/*名片*/

.outBox {
  margin: 0 16px 0 16px;
  width: auto;
  height: auto;
  position: relative;
}

.insideBox {
  padding: 32rpx 32rpx 24rpx 32rpx;
  background: #fff;
  border-radius: 8rpx;
}

.outBox .describle {
  font-size: 34rpx;
  color: #666;
  line-height: 38rpx;
  text-align: left;
  margin-bottom: 32rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}

.allIndustry {
  font-size: 26rpx;
  color: #40587a;
  line-height: 26rpx;
  text-align: left;
  margin-bottom: 24rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.projectDetailLine {
  padding: 0 16rpx;
  border-right: 1rpx solid #999;
}

.projectDetailLight {
  padding: 0 16rpx;
  border-right: 1rpx solid #999;
  color: #40587a;
}

.otherInfo {
  position: relative;
  padding-bottom: 30rpx;
  border-bottom: 1rpx solid #e6e6e6;
}

.others_pic {
  width: 40rpx;
  height: 40rpx;
  display: block;
  margin-right: 16rpx;
}

.projectDetailLine:first-child {
  padding-left: 0;
}

.projectDetailLine:last-child {
  border-right: none;
}

.projectDetailLight:first-child {
  padding-left: 0;
}

.projectDetailLight:last-child {
  border-right: none;
}

.allProjectDetail {
  font-size: 26rpx;
  color: #999;
  line-height: 26rpx;
  text-align: left;
  margin-bottom: 34rpx;
}

.userInfo {
  width: 566rpx;
  margin-top: 9rpx;
}

.userCompany, .userCareer, .userName {
  font-size: 22rpx;
  color: #999;
  line-height: 24rpx;
  text-align: left;
  width: 320rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
}

.userCareer, .userName {
  margin-right: 16rpx;
  white-space: nowrap;
  width: auto;
  text-overflow: clip;
  overflow: none;
}

.projectInfo {
  height: 72rpx;
  position: relative;
}

.seeNum, .haveInterest {
  font-size: 24rpx;
  color: #666;
  line-height: 24rpx;
  text-align: left;
  margin-top: 34rpx;
}

.haveInterest {
  margin-left: 388rpx;
  color: #999;
}

.btnHaveInterest, .btnNoInterest {
  font-size: 24rpx;
  color: #40587a;
  border: 1px solid black;
  height: 52rpx;
  line-height: 52rpx;
  padding: 0 32rpx;
  margin: 20rpx 24rpx 0 185rpx;
}

.btnNoInterest {
  margin: 20rpx 0 0 0;
}

#time {
  font-size: 20rpx;
  color: #999;
  line-height: 20rpx;
  text-align: right;
  margin: 10rpx 0 20rpx 0;
}

/*==========我申请查看的项目===============  */

.apply {
  font-size: 24rpx;
  color: #40587a;
  line-height: 24rpx;
  position: absolute;
  top: 34rpx;
  right: 20rpx;
}

/*=============================  */

