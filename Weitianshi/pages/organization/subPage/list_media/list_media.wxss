page {
  height: 100%;
  /* overflow: hidden; */
}

.media12 {
  width: 100%;
  height: auto;
  background: #fff;
}

.inMedia {
  /* padding: 42rpx 32rpx -2rpx 32rpx;  */
  padding-top: 42rpx;
  /* padding-left: 32rpx;
  padding-right: 32rpx; */
  border-bottom: 1px solid #e6e6e6;
  margin: 0 auto;
  width: 92%;
}

.inMedia:last-child {
  border-bottom: 0;
}

.mediaTime {
  color: #fc703e;
  font-size: 12px;
  margin-bottom: 24rpx;
}

.mediaTitle {
  font-size: 14px;
  color: #333;
  letter-spacing: 0;
  line-height: 45rpx;
  /* height: 88rpx; */
  overflow: hidden;
  text-overflow: -o-ellipsis-lastline;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  margin-bottom: 24rpx;
}

.mediaType {
  font-size: 10px;
  color: #999;
  padding-bottom: 24rpx;
}

.mediaTypeRight {
  margin-right: 10rpx;
}

.mediaLine {
  border-bottom: 1px solid #e6e6e6;
}

.hasNothing {
  margin-top: 410rpx;
  font-size: 36rpx;
  color: #999;
  line-height: 20rpx;
  text-align: center;
  height: 800rpx;
}
