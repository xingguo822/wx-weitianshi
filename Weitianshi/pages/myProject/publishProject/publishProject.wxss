.slect {
  border-radius: 4px;
  height: 82repx;
  line-height: 82rpx;
  border: 1px solid #e0e0e0;
  padding: 0 32rpx;
  justify-content: space-between;
  align-items: baseline;
  padding-right: 24rpx;
}

.slect view {
  display: flex;
  width: 90%;
  color: #ccc;
}

.userBox .checkOn {
  color: #333;
}

picker .checkOn {
  color: #333;
}

.noCheck {
  color: #ccc;
}

.slect image {
  height: 12rpx;
  width: 24rpx;
  display: flex;
}

.tips {
  border: 1px solid #ccc;
  border-radius: 4px;
  width: 30%;
  height: 82rpx;
  line-height: 82rpx;
  text-align: center;
  color: #999;
  font-size: 28rpx;
}

.card .tips_on {
  border: 1px solid #333;
  color: 333;
}

.card_tips .flex {
  justify-content: space-between;
}

.bp {
  display: flex;
  justify-content: space-between;
  padding: 48rpx 0;
}

.bp view:first-child {
  font-size: 24rpx;
  color: #333;
}

.bpBtn {
