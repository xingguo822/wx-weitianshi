.title {
  font-size: 14px;
  color: #666;
  line-height: 80rpx;
  text-align: left;
  background: rgba(255, 241, 232, 0.80);
  width: 95.7%;
  height: 80rpx;
  padding-left: 32rpx;
  margin-bottom: 32rpx;
}

.s_frame {
  height: 1052rpx;
  position: static;
}

.outBox {
  margin: 0 32rpx 24rpx 32rpx;
  border-radius: 8rpx;
}

.projectInfoBox {
  padding: 32rpx;
  background: orangered;
  position: relative;
  background: #fff;
  border-radius: 0 0 8rpx 8rpx;
}

.matching {
  background: #e5e9f2;
  height: 64rpx;
  font-size: 24rpx;
  color: #5e6d82;
  line-height: 64rpx;
  text-align: left;
  padding-left: 32rpx;
   border-radius: 8rpx 8rpx 0 0;
}
.checkedOne {
  border: 1px solid #333;
  color: #333;
}
.matchingNum {
  color: #fc703e;
  letter-spacing: 0;
}

.describle {
  font-size: 32rpx;
  color: #333;
  line-height: 48rpx;
  text-align: left;
  /* width: 480rpx; */
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.industry {
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #666;
  line-height: 26rpx;
  text-align: left;
  margin: 36rpx 0 24rpx 0;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.allProjectDetail {
  font-size: 26rpx;
  color: #666;
  line-height: 26rpx;
  text-align: left;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

.projectDetailType {
  height: 92rpx;
  width: 92rpx;
  right: -3rpx;
  top: -6rpx;
  position: absolute;
}

.projectDetailLine {
  padding: 0 16rpx;
  border-right: 1rpx solid #999;
}

.projectDetailLine:first-child {
  padding-left: 0;
}

.projectDetailLine:last-child {
  border-right: none;
}

/*选中项目的样式  */

.on {
  border: 2rpx solid #40587a;
  border-radius: 8rpx;
}

.pushPro {
  background: #40587a;
  width: 100%;
  height: 54rpx;
  /* padding-top: 34rpx; */
  justify-content: center;
  padding:34rpx 0 0 0;
  line-height:54rpx;
  box-sizing:unset;
  border-radius: 0;
}

.pushProjectAfter {
  font-size: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: #fff;
}
/* .pushProjectAfter {
  font-size: 30rpx;
  line-height: 30rpx;
  text-align: center;
  color: #fff;
} */

/* 错误提示*/

.error {
  background: rgba(0, 0, 0, 0.80);
  border: 1px solid #cbc3a5;
  border-radius: 8rpx;
  font-size: 24rpx;
  color: #666;
  text-align: center;
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  padding: 16rpx 32rpx;
}

/*为空的时候的样式  */

.hasNone {
  text-align: center;
}

.iconStyle {
  background: #fff;
  width: 94rpx;
  height: 118rpx;
  margin-top: 340rpx;
}

.none {
  font-size: 26rpx;
  color: #999;
  line-height: 30rpx;
  margin: 50rpx 0 32rpx 0;
}

.createProject {
  font-size: 30rpx;
  color: #009eff;
  line-height: 30rpx;
}

.checkBoxOut {
  position: relative;
}

.checkbox {
  position: absolute;
  right: -9rpx;
  top: -55rpx;
  z-index: 5;
}

.tips .checkedOne {
  border: 1px solid #333;
  color: #fff;
  background: #40587a;
}

.checkedOne {
  border: 1px solid #333;
  color: #333;
}

.error {
  background: #fef1d3;
  border: 1px solid #cbc3a5;
  border-radius: 4px;
  font-size: 24rpx;
  color: #666;
  text-align: center;
  position: fixed;
  bottom: 100rpx;
  left: 50%;
  transform: translateX(-50%);
  padding: 16rpx 32rpx;
}
.error {
  background: rgba(0, 0, 0, 0.80);
  border-radius: 4px;
  text-align: center;
  position: fixed;
  height: 45rpx;
  line-height: 45rpx;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  padding: 16rpx 32rpx;
  font-size: 28rpx;
  color: #fff;
  }
  

button[disabled][type="default"], wx-button[disabled]:not([type]) {
  background: #40587a;
}
