.topPic {
  height: 46rpx;
  width: 64rpx;
}

.top {
  height: 46rpx;
  width: 64rpx;
  position: absolute;
  right: 26rpx;
  bottom: 30rpx;
}

.nullState {
  position: absolute;
  top: 642rpx;
  left: 302rpx;
  font-size: 36rpx;
  color: #999;
}

.t-highBox {
  display: none;
}

/*空状态  */

.emptyStatus {
  font-size: 28rpx;
  color: #999;
  letter-spacing: 0;
  text-align: center;
  padding-top: 324rpx;
  z-index:-9; 
}
