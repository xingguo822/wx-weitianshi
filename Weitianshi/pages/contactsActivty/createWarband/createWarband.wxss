.userBox, .investBox {
    width: 100%;
    height: 114rpx;
    background: #fff;
    line-height: 112rpx;
}

.arrowStyle {
    margin-top: 47rpx;
    width: 12rpx;
    height: 20rpx;
}

.personListTile {
    font-size: 32rpx;
    color: #333;
}

.personListContentAfter {
    font-size: 28rpx;
    color: #666;
    margin-right: 12rpx;
    text-align: right;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 480rpx;
}

.personListContent {
    font-size: 28rpx;
    color: #ccc;
    text-align: right;
    margin-right: 12rpx;
    text-overflow: ellipsis;
    white-space: nowrap;
    overflow: hidden;
    width: 480rpx;
}

.warName, .createName, .warLogo {
    justify-content: space-between;
    margin: 0 32rpx;
    border-bottom: 1rpx solid #e6e6e6;
    height: auto;
}

.createName {
    border-bottom: none;
}

.illustration {
    margin: 56rpx 20rpx 0 28rpx;
    font-size: 24rpx;
    color: #666;
    line-height: 40rpx;
}

.attention {
    margin-top: 56rpx;
    font-size: 24rpx;
    color: #666;
    line-height: 40rpx;
}

.pushPro {
    background: #40587a;
    width: 100%;
    height: 54rpx;
    justify-content: center;
    padding: 34rpx 0 0 0;
    line-height: 54rpx;
    box-sizing: unset;
    border-radius: 0;
}

.pushProjectAfter {
    font-size: 30rpx;
    line-height: 30rpx;
    text-align: center;
    color: #fff;
}

.uploadAfter {
    width: 64rpx;
    height: 64rpx;
    border: 4rpx;
    margin-top: 10rpx;
}

.uploadBefore {
    width: 28rpx;
    height: 28rpx;
    border: 4rpx;
    margin-top: 50rpx;
    margin-right:20rpx;
}
